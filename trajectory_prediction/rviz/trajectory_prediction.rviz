Panels:
  - Class: rviz/Displays
    Name: Displays
  - Class: rviz/Views
    Name: Views
Visualization Manager:
  Class: ""
  Displays:
    - Alpha: 0.5
      Cell Size: 1
      Class: rviz/Grid
      Color: 160; 160; 164
      Enabled: true
      Name: Grid
      Normal Cell Count: 0
      Offset:
        X: 0
        Y: 0
        Z: 0
      Plane: XY
      Plane Cell Count: 24
      Reference Frame: <Fixed Frame>

    - Class: rviz/MarkerArray
      Enabled: true
      Name: Predicted Paths
      Marker Topic: /trajectory_prediction/predicted_paths
      Queue Size: 100
      Namespaces: {}

    - Class: rviz/MarkerArray
      Enabled: true
      Name: Observation Paths
      Marker Topic: /trajectory_prediction/observation_paths
      Queue Size: 100
      Namespaces: {}

    - Class: rviz/MarkerArray
      Enabled: true
      Name: Agent Markers
      Marker Topic: /trajectory_prediction/agent_markers
      Queue Size: 100
      Namespaces: {}

    - Class: rviz/MarkerArray
      Enabled: true
      Name: PedSim Obstacles
      Marker Topic: /pedsim_simulator/walls
      Queue Size: 100
      Namespaces: {}

  Global Options:
    Background Color: 48; 48; 48
    Fixed Frame: odom
    Frame Rate: 30
  Name: root
  Tools:
    - Class: rviz/Interact
    - Class: rviz/MoveCamera
    - Class: rviz/Select
  Value: true
  Views:
    Current:
      Class: rviz/TopDownOrtho
      Enable Stereo Rendering:
        Stereo Eye Separation: 0.06
        Stereo Focal Distance: 1
        Swap Stereo Eyes: false
        Value: false
      Name: Current View
      Near Clip Distance: 0.01
      Scale: 30
      Target Frame: <Fixed Frame>
      X: 0
      Y: 0
    Saved: ~
Window Geometry:
  Height: 900
  Width: 1200
  X: 100
  Y: 100
